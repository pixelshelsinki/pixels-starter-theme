{
  "name": "pixels-starter-theme",
  "version": "1.3.1",
  "author": "Pixels Helsinki Oy <hello@pixels.fi>",
  "homepage": "https://pixels.fi/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/pixelshelsinki/pixels-starter-theme.git"
  },
  "bugs": {
    "url": "https://github.com/pixelshelsinki/pixels-starter-theme/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "start": "webpack --mode='development' --config assets/build/webpack.config.js",
    "build": "webpack --mode='production' --config assets/build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint assets/scripts assets/build views --config assets/build/.eslintrc.js",
    "lint:styles": "stylelint \"{assets/styles,views}/**/*.{css,sass,scss,sss,less}\" --config assets/build/.stylelintrc.js",
    "fix:scripts": "eslint assets/scripts assets/build views --fix --config assets/build/.eslintrc.js",
    "fix:styles": "stylelint \"{assets/styles,views}/**/*.{css,sass,scss,sss,less}\" --fix --config assets/build/.stylelintrc.js",
    "jest": "jest --verbose --runInBand --passWithNoTests",
    "test": "npm run -s lint && npm run jest",
    "e2e": "yarn cypress run --env env=test",
    "e2e:production": "yarn cypress run --env env=production"
  },
  "engines": {
    "node": ">= 12.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@svgr/webpack": "^5.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "cypress": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "extract-loader": "^4.0.3",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^26.4.2",
    "jest-svg-transformer": "^1.0.0",
    "node-sass": "^4.13.1",
    "postcss-clean": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.2.1",
    "terser-webpack-plugin": "^2.3.4",
    "twig-testing-library": "^1.1.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0",
    "yargs": "^15.1.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
